{
  "name": "modern-face-api",
  "version": "0.22.4",
  "description": "JavaScript API for face detection and face recognition in the browser with tensorflow.js",
  "module": "./build/es6/index.js",
  "main": "./build/commonjs/index.js",
  "typings": "./build/commonjs/index.d.ts",
  "files": [
    "build/commonjs/",
    "dist/modern-face-api.min.js",
    "weights/"
  ],
  "scripts": {
    "rollup-min": "rollup -c rollup.config.mjs --environment minify:true",
    "rollup": "rollup -c rollup.config.mjs",
    "tsc": "tsc",
    "tsc-es6": "tsc --p tsconfig.es6.json",
    "build": "npx rimraf build dist && npm run rollup && npm run rollup-min && npm run tsc && npm run tsc-es6",
    "test": "npm run test-browser && npm run test-node",
    "test-browser": "karma start --single-run",
    "test-node": "ts-node -r ./test/env.node.ts node_modules/jasmine/bin/jasmine --config=jasmine-node.js",
    "test-facelandmarknets": "npx cross-env UUT=faceLandmarkNet karma start",
    "test-facerecognitionnet": "npx cross-env UUT=faceRecognitionNet karma start",
    "test-agegendernet": "npx cross-env UUT=ageGenderNet karma start",
    "test-ssdmobilenetv1": "npx cross-env UUT=ssdMobilenetv1 karma start",
    "test-tinyfacedetector": "npx cross-env UUT=tinyFaceDetector karma start",
    "test-globalapi": "npx cross-env UUT=globalApi karma start",
    "test-cpu": "npx cross-env BACKEND_CPU=true karma start",
    "docs": "typedoc --options ./typedoc.config.js ./src",
    "prepare": "husky",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "check:ci": "biome ci .",
    "check:all": "biome check --write . && npm run test",
    "validate": "npm run check:ci && npm run test"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "biome check --write --no-errors-on-unmatched"
    ],
    "*.{json,jsonc}": [
      "biome format --write --no-errors-on-unmatched"
    ]
  },
  "keywords": [
    "face",
    "detection",
    "recognition",
    "tensorflow",
    "tf"
  ],
  "author": "SujalXplores",
  "bugs": {
    "url": "https://github.com/SujalXplores/modern-face-api/issues"
  },
  "repository": {
    "url": "https://github.com/SujalXplores/modern-face-api",
    "type": "git"
  },
  "sideEffects": false,
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-core": "4.22.0",
    "@tensorflow/tfjs-backend-cpu": "4.22.0",
    "@tensorflow/tfjs-backend-webgl": "4.22.0",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "@types/jasmine": "5.1.9",
    "@types/node": "24.3.0",
    "@web/test-runner": "0.20.2",
    "@web/test-runner-playwright": "0.11.1",
    "canvas": "3.2.0",
    "cross-env": "10.0.0",
    "esbuild": "0.25.9",
    "husky": "9.1.7",
    "jasmine": "5.10.0",
    "jasmine-core": "5.10.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-esbuild": "2.3.0",
    "karma-jasmine": "5.1.0",
    "karma-typescript": "5.5.4",
    "lint-staged": "16.1.5",
    "mocha": "11.7.1",
    "rimraf": "6.0.1",
    "rollup": "4.50.0",
    "rollup-plugin-typescript2": "0.36.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  }
}
